/**
 * Show the example of single level inheritance with constructor invocation.
 */

/**
 * @author Vishal
 *
 */
class base {
	int c1;
	base(){
		this.c1=39;
		System.out.println("Default constructor of base");
	}
	
	int getC1() {
		return c1;
	}
	
}

class sub extends base {
	int c2;
	sub(){
		this.c2=44;
		System.out.println("Default constructor of sub");
	}
	
}

public class Demo{
	
} 