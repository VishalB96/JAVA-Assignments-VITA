class base{
	int n=23;
	
	void disp() {
		System.out.println("disp base");
	}
}
class sub extends base{
	void disp() {
		System.out.println("disp sub");
	}
}
public class Demo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		new sub().disp();
		
		new base().disp();
	}

}
