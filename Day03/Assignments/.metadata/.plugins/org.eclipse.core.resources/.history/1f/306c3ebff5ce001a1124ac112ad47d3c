class base{

	/**
	 * 
	 */
	public base() {
		super();
		// TODO Auto-generated constructor stub
	}
	
}
class sub1 extends base{

	/**
	 * 
	 */
	public sub1() {
		super();
		// TODO Auto-generated constructor stub
	}
	
}
class sub2 extends base{

	/**
	 * 
	 */
	public sub2() {
		super();
		// TODO Auto-generated constructor stub
	}
	
}
public class Demo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		new base();
		System.out.println("--------------------");
		new sub2();
		System.out.println("----------------------");
		new sub1();
		
	}

}
